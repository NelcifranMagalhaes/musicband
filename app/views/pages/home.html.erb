<% if @band.present? %>  
	<section class="hero">
		<% @band.photos.each do |ph| %>
			<div class="mySlides fade">
				<figure class="image is-2by1">
					<%= cl_image_tag ph.image.filename  + ".jpg",secure: true%>
				</figure>
			</div>
		<% end %>
	</section>
		<br>
<% end %>
<%= render partial: "band",object: @artists %>
<%= render partial: "discography",object: @discographies %>
<%= render partial: "tour" ,object: @tours %>

